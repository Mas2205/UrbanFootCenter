# Configuration de l'environnement
NODE_ENV=production
PORT=5001

# JWT - GÉNÉRER UNE CLÉ FORTE DE 64 CARACTÈRES
JWT_SECRET=CHANGEZ_MOI_AVEC_UNE_CLE_FORTE_64_CARACTERES
JWT_EXPIRES_IN=24h

# Base de données - UTILISER DES MOTS DE PASSE FORTS
DB_HOST=localhost
DB_PORT=5432
DB_NAME=urban_foot_center
DB_USER=postgres
DB_PASSWORD=your-strong-database-password-here
DB_SYNC=false

# CORS - SPÉCIFIER VOS DOMAINES EN PRODUCTION
CORS_ORIGIN=https://votre-domaine.com,https://www.votre-domaine.com

# Mail - CONFIGURER AVEC VOS VRAIES CREDENTIALS
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USER=VOTRE_EMAIL@EXAMPLE.COM
MAIL_PASS=VOTRE_MOT_DE_PASSE_APP
MAIL_FROM=no-reply@votre-domaine.com

# SMS (Twilio) - REMPLACER PAR VOS VRAIES CLÉS
TWILIO_ACCOUNT_SID=your_real_account_sid
TWILIO_AUTH_TOKEN=your_real_auth_token
TWILIO_PHONE_NUMBER=+221xxxxxxxxx

# Paiement - UTILISER LES CLÉS DE PRODUCTION
STRIPE_PUBLIC_KEY=pk_live_your_real_key
STRIPE_SECRET_KEY=sk_live_your_real_key

# PayDunya (Marketplace) - REMPLACER PAR VOS VRAIES CLÉS
PAYDUNYA_MASTER_KEY=VOTRE_MASTER_KEY_PAYDUNYA
PAYDUNYA_PRIVATE_KEY=VOTRE_PRIVATE_KEY_PAYDUNYA
PAYDUNYA_PUBLIC_KEY=VOTRE_PUBLIC_KEY_PAYDUNYA

# Wave Payout API - REMPLACER PAR VOTRE VRAIE CLÉ
WAVE_API_KEY=wave_sn_prod_VOTRE_CLE_WAVE

# Configuration Marketplace
PAYMENTS_ENV=sandbox
PLATFORM_FEE_BPS=1000
BASE_URL=http://localhost:5001
WAVE_API_KEY=your_real_wave_api_key
ORANGE_MONEY_API_KEY=your_real_orange_money_api_key

# AWS S3 - CONFIGURER POUR VOS UPLOADS
AWS_ACCESS_KEY_ID=your_real_access_key
AWS_SECRET_ACCESS_KEY=your_real_secret_key
AWS_REGION=eu-west-1
AWS_BUCKET_NAME=urban-foot-center-prod

# Firebase Cloud Messaging
FCM_SERVER_KEY=your_real_fcm_server_key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Session Security
SESSION_SECRET=your-super-secure-session-secret-64-characters-minimum
